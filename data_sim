from enum import Enum
from dataclasses import dataclass, field
from typing import List, Optional


class Major(Enum):
    STSDS = 1  # Statistics and Data Science
    AMATH = 2  # Applied Mathematics
    ECON = 3  # Economics
    CMPSC = 4  # Computer Science


@dataclass
class Course:
    id: str
    full_title: str
    prerequisite: List["Course"] = field(default_factory=list)


